{
   "version": 2,

    "builds": [
      {
        "src": "manage.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "manage.py"
      }
    ],

    "env": {
      
       "DJANGO_SECRET_KEY": "@django_secret_key",
       "DATABASE_URL": "@database_url",
       "ALLOWED_HOSTS": "@allowed_hosts",
       "DEBUG": "False"
    }
   
  }