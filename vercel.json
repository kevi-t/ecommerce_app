{
    "version": 2,
    "builds": [
      {
        "src": "config/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles_build/static/$1"
      },
      {
        "src": "/static/css/(.*)",
        "dest": "/staticfiles_build/static/css/$1"
      },
      {
        "src": "/(.*)",
        "dest": "config/wsgi.py"
      }
    ]
  }